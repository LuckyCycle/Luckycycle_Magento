<config>
    <modules>
        <Luckycycle_Snippetsdk>
            <version>1.0.0</version>
        </Luckycycle_Snippetsdk>
    </modules>
    <frontend>
        <routers>
          <snippetsdk>
            <use>standard</use>
              <args>
                <module>Luckycycle_Snippetsdk</module>
                <frontName>luckycycle</frontName>
              </args>
          </snippetsdk>
        </routers>
        <layout>
            <updates>
                <snippetsdk>
                    <file>Luckycycle/snippetsdk.xml</file>
                </snippetsdk>
            </updates>
        </layout>
    </frontend>
    <global>
        <helpers>
           <snippetsdk>
               <class>Luckycycle_Snippetsdk_Helper</class>
           </snippetsdk>
        </helpers>
        <blocks>
          <snippetsdk>
            <class>Luckycycle_Snippetsdk_Block</class>
          </snippetsdk>
        </blocks>
    </global>
    <default>
        <snippetsdk>
            <params>
                <enabled>0</enabled>
                <use_iframe>1</use_iframe>
            </params>
        </snippetsdk>
    </default>
</config>